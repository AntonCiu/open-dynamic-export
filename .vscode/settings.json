{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "json.schemas": [
        {
            "fileMatch": ["/config/*.json"],
            "url": "/config.schema.json"
        }
    ],
    "files.readonlyInclude": {
        "**/routeTree.gen.ts": true
    },
    "files.watcherExclude": {
        "**/routeTree.gen.ts": true
    },
    "search.exclude": {
        "**/routeTree.gen.ts": true
    }
}