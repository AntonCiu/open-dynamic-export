        "jsrsasign": "^11.1.0",
        "modbus-serial": "^8.0.17",
        "mqtt": "^5.10.1",
        "openapi-fetch": "^0.12.0",
        "pino": "^9.4.0",
        "pino-pretty": "^11.2.2",
        "pino-rotating-file-stream": "^0.0.2",
